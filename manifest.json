{
	"name": "Emoji Box",
	"version": "1.2.86.1405",
	"manifest_version": 2,
	"description": "Input emoji.",
	"author": "Yulon",
	"homepage_url": "https://github.com/yulon/emoji-box",
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"<all_urls>"
	],
	"background": {
		"scripts": ["bext.js", "bg.js"]
	}
}
