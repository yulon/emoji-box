{
	"name": "Emoji Box",
	"version": "0.5.64",
	"manifest_version": 2,
	"description": "Input emoji.",
	"homepage_url": "https://github.com/yulon/emoji-box",
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"activeTab",
		"contextMenus",
		"http://*/*",
		"https://*/*"
	],
	"background": {
        "scripts": ["menu.js"]
    },
	"content_scripts": [{
		"js": ["emojis.js", "view.js"],
		"css": ["style.css"],
		"matches": [ "http://*/*", "https://*/*" ],
		"run_at": "document_start"
	}]
}
